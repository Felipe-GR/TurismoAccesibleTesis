{
  "expo": {
    "name": "Museo Nacional - Accesible",
    "description": "Esta aplicación es una guia para la exposición permanente del museo nacional.",
    "slug": "Prueba-Museo-Nacional",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/iconBadgerFox.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "co.MuseoNacional.Accesible"
    },
    "android": {
      "package": "co.MuseoNacional.Accesible"
    }
  }
}
